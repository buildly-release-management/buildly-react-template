# Example .env file for local development
# Build environment
build=local
# Base API URL for Buildly Core
API_URL=https://labs-api.buildly.dev

# OAuth Configuration
OAUTH_CLIENT_ID=your-local-oauth-client-id
OAUTH_TOKEN_URL=https://labs-api.buildly.dev/oauth/token

# GitHub Integration 

# Trello Integration
TRELLO_API_KEY=your-trello-api-key

# Feedback Configuration
FEEDBACK_SHEET=https://docs.google.com/spreadsheets/d/your-feedback-sheet-id

# Product Service Configuration
PRODUCT_SERVICE_URL=https://labs-api.buildly.dev
PRODUCT_SERVICE_TOKEN=your-product-service-token

# Release Service Configuration
RELEASE_SERVICE_URL=https://labs-api.buildly.dev
RELEASE_SERVICE_TOKEN=your-release-service-token

# Miscellaneous
FREE_COUPON_CODE=your-free-coupon-code
PRODUCTION=false

# Instructions:
# 1. Copy this file to `.env` in the root of your project.
# 2. Replace the placeholder values (e.g., `your-local-oauth-client-id`) with actual values for your local environment.
# 3. Save the file and restart your development server to apply the changes.