    "build": "webpack --mode development --openssl-legacy-provider",
    "start": "webpack-dev-server --mode development --openssl-legacy-provider",
    "build:dev": "webpack --mode development --env.build=dev --openssl-legacy-provider",
    "start:dev": "webpack-dev-server --mode development --env.build=dev --openssl-legacy-provider",
    "https:local": "webpack-dev-server --https --ca ~/.localhost-ssl/localhost.pem --mode development --env.build=local",
    "start:local": "webpack-dev-server --mode development --env.build=local --openssl-legacy-provider",
    "build:prod": "webpack --mode production --env.build=prod --openssl-legacy-provider",
    "start:prod": "webpack-dev-server --mode production --env.build=prod",
