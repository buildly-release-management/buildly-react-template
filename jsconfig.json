{
  "extends": "./jsconfig.paths.json",
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "paths": {
    "@assets/*": ["./src/assets/*"],
    "@components/*": ["./src/components/*"],
    "@redux/*": ["./src/redux/*"]
  }
}